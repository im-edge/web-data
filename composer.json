{
    "name": "imedge/web-data",
    "description": "IMEdge Web Data library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Gelf",
            "email": "thomas@gelf.net"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "IMEdge\\Web\\Data\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IMEdge\\Tests\\Web\\Data\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "imedge/web-select2": "@dev"
    },
    "require-dev": {
        "gipfl/zfdb": ">=0.4",
        "gipfl/zfdbstore": ">=0.3",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.3 || ^9.5",
        "psr/log": "^1.0 || ^3.0",
        "ramsey/uuid": ">=4.2",
        "squizlabs/php_codesniffer": "^3.7",
        "symplify/easy-coding-standard": "^12.0.8"
    }
}
